<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="250dp"
    android:background="@drawable/autocomplete_textview_bg"
    android:layout_marginVertical="5dp"
    android:layout_marginHorizontal="5dp"
    android:backgroundTint="#DBDBDC">

    <TextView
        android:id="@+id/name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name_"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/lastname_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last name"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/name_text"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/lastname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="last Name_"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/name"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/dob_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date of Birth"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/lastname_text"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/dob"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dob_"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/lastname"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/doi_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date of Issue"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/dob_text"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/doi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="doi_"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/dob"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/doe_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date of Expiry"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/doi_text"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/doe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="doe_"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/doi"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/identification_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Identification Number"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/doe_text"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"/>

    <TextView
        android:id="@+id/identification"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="identification_"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/doe"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"/>



</RelativeLayout>